# Plot actual vs predicted prices
plt.figure(figsize=(14, 7))
plt.plot(test_data.index, test_data, label='Actual Price', color='b')
plt.plot(test_data.index, predictions, label='Predicted Price', color='r')
plt.title('Stock Price Prediction')
plt.xlabel('Date')
plt.ylabel('Price')
plt.legend()
plt.show()
